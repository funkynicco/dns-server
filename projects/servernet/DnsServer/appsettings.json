{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "SetHostMiddleware": {
      "Hostname": null,
      "IsReverseProxyHttps": false
    },
    "DnsServer": {
      "Enabled": true,
      "BindAddress": "0.0.0.0",
      "BindPort": 53,
      "Proxy": {
        "Address": "8.8.8.8",
        "Port": 53
      }
    }
  }
}
